  }
  union {
   #declare KNOB_X = -4.5;
   #while (KNOB_X<5)
    #declare KNOB_Y = -9.5;
    #while (KNOB_Y<10)
     object {
      lg_knob_inner_space
      translate <KNOB_X*LG_BRICK_WIDTH, KNOB_Y*LG_BRICK_WIDTH, -LG_TOP_HEIGHT>
     }
     #declare KNOB_Y = KNOB_Y + 1;
    #end
    #declare KNOB_X = KNOB_X + 1;
   #end
  }
 }
 #declare KNOB_X = -4.5;
 #while (KNOB_X<5)
  #declare KNOB_Y = -9.5;
  #while (KNOB_Y<10)
   object {
    lg_knob
    translate <KNOB_X*LG_BRICK_WIDTH, KNOB_Y*LG_BRICK_WIDTH, 0>
   }
   #declare KNOB_Y = KNOB_Y + 1;
  #end
  #declare KNOB_X = KNOB_X + 1;
 #end
}

